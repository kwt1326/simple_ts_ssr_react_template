{
  "name": "front-end",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build:dev && ts-node dist/server-bundle.js",
    "start-dev": "NODE_ENV=development webpack && nodemon --exec ts-node dist/server-bundle.js",
    "build:prod": "NODE_ENV=production npm run build:prod-client && NODE_ENV=production npm run build:prod-server",
    "build:dev": "NODE_ENV=development npm run build:client && NODE_ENV=development npm run build:server",
    "build:server": "webpack --config webpack/webpack.server.js --env env=server",
    "build:client": "webpack --config webpack/webpack.client.js --env env=client",
    "build:prod-server": "webpack -p --config webpack/webpack.server.js --env env=server",
    "build:prod-client": "webpack -p --config webpack/webpack.client.js --env env=client"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/register": "^7.12.10",
    "express": "^4.17.1",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "typescript": "^4.1.3",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/express": "^4.17.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.15",
